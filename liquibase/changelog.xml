            <column name="is2FAEnabled" type="boolean"/>
            <column name="planId" type="int" />
            <column name="codeExpiredIn" type="datetime"/>
            <column name="createdAt" type="datetime" defaultValueComputed="now()" >
                <constraints nullable="false"/>
            </column>
            <column name="updatedAt" type="datetime" defaultValueComputed="now()"/>
        </createTable>
        <createIndex indexName="idx_email" tableName="users">
            <column name="email"/>
        </createIndex>
        <createIndex indexName="idx_country" tableName="users">
            <column name="country"/>
        </createIndex>
        <createIndex indexName="idx_isActive" tableName="users">
            <column name="isActive"/>
        </createIndex>
        <createIndex indexName="idx_isAdmin" tableName="users">
            <column name="isAdmin"/>
        </createIndex>
        <createIndex indexName="idx_companyName" tableName="users">
            <column name="companyName"/>
        </createIndex>
        <createIndex indexName="idx_planId" tableName="users">
            <column name="planId"/>
        </createIndex>
        <rollback>
            <dropTable tableName="user"/>
        </rollback>
    </changeSet>
