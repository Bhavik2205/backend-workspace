    <changeSet author="Dharmik Dalwadi" id="1">
        <createTable tableName="users">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="firstName" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="lastName" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="email" type="varchar(255)">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="password" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="mobile" type="varchar(25)">
            </column>
            <column name="country" type="varchar(100)"/>
            <column name="joinDate" type="date"/>
            <column name="isActive" type="boolean" defaultValueComputed="true">
                <constraints nullable="false"/>
            </column>
            <column name="isAdmin" type="boolean" defaultValueComputed="false">
                <constraints nullable="false"/>
            </column>
            <column name="companyName" type="varchar(100)"/>
